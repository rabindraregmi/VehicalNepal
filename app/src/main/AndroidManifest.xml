<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rabindra.vehicalnepal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".Activities.HomeActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.MainActivity"
            android:label="Vehical Nepal"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activities.BrandListActivity" />
        <activity
            android:name=".Activities.BikeListItemActivity"
            android:label="@string/title_activity_bike_list_item"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activities.BikeListActivity" />
        <activity android:name=".Activities.CarListActivity" />
        <activity android:name=".Activities.BikeDetailPageActivity" />
        <activity android:name=".Activities.CarDetailPageActivity" />
        <activity android:name=".Activities.BikeHomePageActivity" />
        <activity android:name=".Activities.CarBrandListActivity" />
        <activity android:name=".Activities.BikeCompareActivity" />
        <activity android:name=".Activities.CarCompareActivity"></activity>
    </application>

</manifest>